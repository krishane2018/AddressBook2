<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
    <bean id="addressBookView" class="AddressBookView">
        <constructor-arg index="0" ref="addressBook"/>
        <constructor-arg index="1" ref="menuController"/>
    </bean>
    <bean id="menuController" class="MenuController">
        <constructor-arg index="0" ref="addressBook"/>
    </bean>
    <bean id="addressBook" class="AddressBook">
    </bean>
</beans>